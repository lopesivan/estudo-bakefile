# -------------------------------------------------------------------------

# Standard flags for CC 
CFLAGS ?= 

# Standard preprocessor flags (common for CC and CXX) 
CPPFLAGS ?= 

# Standard linker flags 
LDFLAGS ?= 



# -------------------------------------------------------------------------
# Do not modify the rest of this file!
# -------------------------------------------------------------------------

### Variables: ###

CPPDEPS = -MT$@ -MF`echo $@ | sed -e 's,\.o$$,.d,'` -MD -MP
HELLO_CFLAGS =  -Isrc  $(CPPFLAGS) $(CFLAGS)
HELLO_OBJECTS =  \
	hello_main.o

### Conditionally set variables: ###



### Targets: ###

all: hello

install: 

uninstall: 

clean: 
	rm -f ./*.o
	rm -f ./*.d
	rm -f hello

hello: $(HELLO_OBJECTS)
	clang -o $@ $(HELLO_OBJECTS)  $(LDFLAGS)

hello_main.o: ./src/main.c
	clang -c -o $@ $(HELLO_CFLAGS) $(CPPDEPS) $<

.PHONY: all install uninstall clean


# Dependencies tracking:
-include ./*.d
