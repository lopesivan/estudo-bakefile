# -------------------------------------------------------------------------
# These are configurable options:
# -------------------------------------------------------------------------

# Standard flags for CC 
CFLAGS ?= 

# Standard preprocessor flags (common for CC and CXX) 
CPPFLAGS ?= 

# Standard linker flags 
LDFLAGS ?= 



# -------------------------------------------------------------------------
# Do not modify the rest of this file!
# -------------------------------------------------------------------------

### Variables: ###

CPPDEPS = -MT$@ -MF$@.d -MD -MP
HELLO_CFLAGS =  -Isrc  $(CPPFLAGS) $(CFLAGS)
HELLO_OBJECTS =  \
	hello_main.o

### Conditionally set variables: ###



### Targets: ###

all: hello.exe

clean: 
	-if exist .\*.o del .\*.o
	-if exist .\*.d del .\*.d
	-if exist hello.exe del hello.exe

hello.exe: $(HELLO_OBJECTS)
	$(foreach f,$(subst \,/,$(HELLO_OBJECTS)),$(shell echo $f >> $(subst \,/,$@).rsp.tmp))
	@move /y $@.rsp.tmp $@.rsp >nul
	x86_64-w64-mingw32-gcc -o $@ @$@.rsp  $(LDFLAGS)
	@-del $@.rsp

hello_main.o: ./src/main.c
	x86_64-w64-mingw32-gcc -c -o $@ $(HELLO_CFLAGS) $(CPPDEPS) $<

.PHONY: all clean


SHELL := $(COMSPEC)

# Dependencies tracking:
-include ./*.d
